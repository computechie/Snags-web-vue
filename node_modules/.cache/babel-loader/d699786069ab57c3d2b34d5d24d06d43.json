{"ast":null,"code":"// Primevue datatable help: https://www.primefaces.org/primevue/datatable //\nimport DataTable from \"primevue/datatable\";\nimport Column from \"primevue/column\";\nimport InputText from 'primevue/inputtext'; //import Checkbox from 'primevue/checkbox';\n//import Button from 'primevue/button';\n//import Dialog from 'primevue/dialog';\n//import InlineMessage from 'primevue/inlinemessage';\n//import InputNumber from 'primevue/inputnumber';\n//import TabView from 'primevue/tabview';\n//import TabPanel from 'primevue/tabpanel';\n//import Dropdown from 'primevue/dropdown';\n\nimport axios from \"axios\";\nimport $ from \"jquery\";\nimport { FilterMatchMode, FilterOperator } from 'primevue/api'; // for filtering\n\nexport default {\n  props: [\"systemId\"],\n  components: {\n    DataTable,\n    Column,\n    InputText //Checkbox,\n    //Button,\n    //Dialog,\n    //InlineMessage,\n    //InputNumber,\n    //TabView,\n    //TabPanel,\n    //Dropdown,\n\n  },\n\n  data() {\n    return {\n      userType: '',\n      isLoading: false,\n      tableHeight: 'calc( 100vh - 215px)',\n      allUsers: [],\n      editUserName: '',\n      editUserSurname: '',\n      editUserEmail: '',\n      editUserType: '',\n      editUserActive: false,\n      filters: {\n        'global': {\n          value: null,\n          matchMode: FilterMatchMode.CONTAINS\n        },\n        'name': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'surname': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'email': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'isActive': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        }\n      },\n      matchModes: [{\n        label: 'Starts With',\n        value: FilterMatchMode.STARTS_WITH\n      }, {\n        label: 'Contains',\n        value: FilterMatchMode.CONTAINS\n      }]\n    };\n  },\n\n  mounted() {\n    this.userType = this.$store.getters.getUserType.toUpperCase();\n    this.getAllUsers();\n    this.moveSearchBoxToTop();\n  },\n\n  methods: {\n    reorganizePagination() {\n      $(\".p-paginator-current\").detach().appendTo('.p-paginator-left-content');\n      $(\".p-paginator-rpp-options\").detach().appendTo('.p-paginator-right-content');\n    },\n\n    moveSearchBoxToTop() {\n      //default search box is inside header of the datatable, must move to the top of the page\n      $('#hereMoveSearch').empty();\n      $(\"#searchBox\").detach().appendTo('#hereMoveSearch');\n    },\n\n    onRowClick(event) {\n      // alert( event.data.id);\n      this.ClickedRowId = event.data.id; // key for update\n\n      this.editUserName = event.data.name;\n      this.editUserSurname = event.data.surname;\n      this.editUserEmail = event.data.email;\n      this.editUserType = event.data.type;\n      this.editUserActive = event.data.isActive;\n      $('.hiddenFields').each(function () {\n        $(this).closest('tr').removeClass('highlight');\n      });\n      $('.hiddenFields').each(function () {\n        if ($(this).attr('data') == event.data.id) {\n          $(this).closest('tr').addClass('highlight');\n        }\n      });\n    },\n\n    onRowSelect(event) {//here open panel, but for manage users don't need to open panel\n    },\n\n    getAllUsers() {\n      this.isLoading = true;\n      const sessionId = localStorage.getItem(\"token\");\n      const baseUrl = localStorage.getItem(\"_rootRestUrl\");\n      let formData = JSON.stringify({\n        sessionId: sessionId\n      });\n      let config = {\n        headers: {\n          \"Content-Type\": \"application/json\"\n        }\n      };\n      axios.get(baseUrl + \"/api/v1/User/Users/?sessionId=\" + sessionId + \"\", formData, config).then(response => {\n        this.allUsers = response.data;\n        this.reorganizePagination();\n        this.isLoading = false;\n      }).catch(function (error) {\n        // handle error\n        if (error.response.status == '401') {\n          //not authorized, token expires\n          //hm, I don't know why this three next lines not work :( so I made it \"manual logout\"\n          // this.isLoading = false;  \n          // this.$store.dispatch(\"logout\");\n          // this.$router.replace(\"/\");\n          localStorage.removeItem(\"token\");\n          localStorage.removeItem(\"userId\");\n          localStorage.removeItem(\"userFullname\");\n          localStorage.removeItem(\"userType\");\n          localStorage.removeItem(\"tokenExpiration\");\n          localStorage.removeItem(\"expiresIn\");\n          document.location = '/';\n          this.isLoading = false;\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoJA;AAEA,OAAOA,SAAP,MAAsB,oBAAtB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AACA,OAAOC,SAAP,MAAsB,oBAAtB,C,CACA;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,CAAP,MAAc,QAAd;AAEA,SAAQC,eAAR,EAAwBC,cAAxB,QAA6C,cAA7C,C,CAAqE;;AAIrE,eAAe;EACXC,KAAK,EAAE,CAAC,UAAD,CADI;EAEXC,UAAU,EAAE;IACRR,SADQ;IAERC,MAFQ;IAGRC,SAHQ,CAIR;IACA;IACA;IACA;IACA;IACA;IACA;IACA;;EAXQ,CAFD;;EAiBbO,IAAI,GAAG;IACD,OAAO;MACHC,QAAO,EAAI,EADR;MAEHC,SAAS,EAAC,KAFP;MAGHC,WAAW,EAAC,sBAHT;MAIHC,QAAQ,EAAC,EAJN;MAMHC,YAAY,EAAC,EANV;MAOHC,eAAe,EAAC,EAPb;MAQHC,aAAa,EAAC,EARX;MASHC,YAAY,EAAC,EATV;MAUHC,cAAc,EAAC,KAVZ;MAYHC,OAAO,EAAE;QACN,UAAU;UAACC,KAAK,EAAE,IAAR;UAAcC,SAAS,EAAEhB,eAAe,CAACiB;QAAzC,CADJ;QAEN,QAAO;UAACC,QAAQ,EAAEjB,cAAc,CAACkB,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAEhB,eAAe,CAACiB;UAAzC,CAAD;QAA5C,CAFD;QAGN,WAAW;UAACC,QAAQ,EAAEjB,cAAc,CAACkB,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAEhB,eAAe,CAACiB;UAAzC,CAAD;QAA5C,CAHL;QAIN,SAAS;UAACC,QAAQ,EAAEjB,cAAc,CAACkB,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAEhB,eAAe,CAACiB;UAAzC,CAAD;QAA5C,CAJH;QAKN,YAAY;UAACC,QAAQ,EAAEjB,cAAc,CAACkB,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAEhB,eAAe,CAACiB;UAAzC,CAAD;QAA5C;MALN,CAZN;MAoBHI,UAAU,EAAE,CACJ;QAACC,KAAK,EAAE,aAAR;QAAuBP,KAAK,EAAEf,eAAe,CAACuB;MAA9C,CADI,EAEJ;QAACD,KAAK,EAAE,UAAR;QAAoBP,KAAK,EAAEf,eAAe,CAACiB;MAA3C,CAFI;IApBT,CAAP;EAyBH,CA3CU;;EA4CXO,OAAO,GAAG;IACN,KAAKnB,QAAL,GAAiB,KAAKoB,MAAL,CAAYC,OAAZ,CAAoBC,WAArB,CAAkCC,WAAlC,EAAhB;IACA,KAAKC,WAAL;IACA,KAAKC,kBAAL;EACH,CAhDU;;EAiDXC,OAAO,EAAE;IAERC,oBAAoB,GAAE;MACrBjC,CAAC,CAAC,sBAAD,CAAD,CAA0BkC,MAA1B,GAAmCC,QAAnC,CAA4C,2BAA5C;MACAnC,CAAC,CAAC,0BAAD,CAAD,CAA8BkC,MAA9B,GAAuCC,QAAvC,CAAgD,4BAAhD;IACD,CALQ;;IAOTJ,kBAAkB,GAAE;MAChB;MACA/B,CAAC,CAAC,iBAAD,CAAD,CAAqBoC,KAArB;MACApC,CAAC,CAAC,YAAD,CAAD,CAAgBkC,MAAhB,GAAyBC,QAAzB,CAAkC,iBAAlC;IACH,CAXQ;;IAaTE,UAAU,CAACC,KAAD,EAAQ;MAEd;MACI,KAAKC,YAAL,GAAoBD,KAAK,CAACjC,IAAN,CAAWmC,EAA/B,CAHU,CAGyB;;MAEnC,KAAK9B,YAAL,GAAoB4B,KAAK,CAACjC,IAAN,CAAWoC,IAA/B;MACA,KAAK9B,eAAL,GAAuB2B,KAAK,CAACjC,IAAN,CAAWqC,OAAlC;MACA,KAAK9B,aAAL,GAAqB0B,KAAK,CAACjC,IAAN,CAAWsC,KAAhC;MACA,KAAK9B,YAAL,GAAoByB,KAAK,CAACjC,IAAN,CAAWuC,IAA/B;MACA,KAAK9B,cAAL,GAAsBwB,KAAK,CAACjC,IAAN,CAAWwC,QAAjC;MAIA7C,CAAC,CAAC,eAAD,CAAD,CAAmB8C,IAAnB,CAAwB,YAAW;QAEjC9C,CAAC,CAAC,IAAD,CAAD,CAAQ+C,OAAR,CAAgB,IAAhB,EAAsBC,WAAtB,CAAkC,WAAlC;MACA,CAHF;MAKAhD,CAAC,CAAC,eAAD,CAAD,CAAmB8C,IAAnB,CAAwB,YAAW;QAEjC,IAAG9C,CAAC,CAAC,IAAD,CAAD,CAAQiD,IAAR,CAAa,MAAb,KAAsBX,KAAK,CAACjC,IAAN,CAAWmC,EAApC,EAAuC;UACrCxC,CAAC,CAAC,IAAD,CAAD,CAAQ+C,OAAR,CAAgB,IAAhB,EAAsBG,QAAtB,CAA+B,WAA/B;QACD;MAEF,CAND;IAQH,CAvCI;;IAyCLC,WAAW,CAACb,KAAD,EAAQ,CACf;IACH,CA3CI;;IA6CLR,WAAW,GAAE;MACX,KAAKvB,SAAL,GAAiB,IAAjB;MAEA,MAAM6C,SAAS,GAACC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAhB;MACA,MAAMC,OAAM,GAAIF,YAAY,CAACC,OAAb,CAAqB,cAArB,CAAhB;MAEA,IAAIE,QAAO,GAAIC,IAAI,CAACC,SAAL,CAAe;QAAEN,SAAS,EAAEA;MAAb,CAAf,CAAf;MACA,IAAIO,MAAK,GAAI;QACXC,OAAO,EAAE;UACP,gBAAgB;QADT;MADE,CAAb;MAOC7D,KAAI,CACF8D,GADF,CACMN,OAAM,GAAI,gCAAV,GAA2CH,SAA3C,GAAqD,EAD3D,EAC+DI,QAD/D,EACyEG,MADzE,EAEEG,IAFF,CAEQC,QAAD,IAAc;QACjB,KAAKtD,QAAL,GAAgBsD,QAAQ,CAAC1D,IAAzB;QACA,KAAK4B,oBAAL;QACA,KAAK1B,SAAL,GAAiB,KAAjB;MACF,CANF,EAOGyD,KAPH,CAOS,UAAUC,KAAV,EAAiB;QACrB;QAGA,IAAGA,KAAK,CAACF,QAAN,CAAeG,MAAf,IAAuB,KAA1B,EAAiC;UAAE;UACnC;UAED;UACA;UACA;UAECb,YAAY,CAACc,UAAb,CAAwB,OAAxB;UACAd,YAAY,CAACc,UAAb,CAAwB,QAAxB;UACAd,YAAY,CAACc,UAAb,CAAwB,cAAxB;UACAd,YAAY,CAACc,UAAb,CAAwB,UAAxB;UACAd,YAAY,CAACc,UAAb,CAAwB,iBAAxB;UACAd,YAAY,CAACc,UAAb,CAAwB,WAAxB;UAGAC,QAAQ,CAACC,QAAT,GAAoB,GAApB;UAEA,KAAK9D,SAAL,GAAiB,KAAjB;QACA;MAEF,CA/BH;IAkCH;;EA7FK;AAjDE,CAAf","names":["DataTable","Column","InputText","axios","$","FilterMatchMode","FilterOperator","props","components","data","userType","isLoading","tableHeight","allUsers","editUserName","editUserSurname","editUserEmail","editUserType","editUserActive","filters","value","matchMode","CONTAINS","operator","AND","constraints","matchModes","label","STARTS_WITH","mounted","$store","getters","getUserType","toUpperCase","getAllUsers","moveSearchBoxToTop","methods","reorganizePagination","detach","appendTo","empty","onRowClick","event","ClickedRowId","id","name","surname","email","type","isActive","each","closest","removeClass","attr","addClass","onRowSelect","sessionId","localStorage","getItem","baseUrl","formData","JSON","stringify","config","headers","get","then","response","catch","error","status","removeItem","document","location"],"sourceRoot":"","sources":["Q:\\Projects\\Transport-web-\\src\\components\\pages\\transport\\ManageUsers.vue"],"sourcesContent":["<template>\r\n\r\n  <base-dialog :show=\"isLoading\"  title=\"Authenticating...\" fixed>\r\n    <base-spinner></base-spinner>\r\n  </base-dialog>\r\n\r\n  <div >\r\n  <DataTable :id=\"dataTable\" :value=\"allUsers\" :paginator=\"true\" :rows=\"25\"   dataKey=\"id\"  stripedRows v-model:filters=\"filters\" filterDisplay=\"menu\" key=\"key\" \r\n            paginatorTemplate=\"CurrentPageReport FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink RowsPerPageDropdown\"\r\n            paginatorStart=\"CurrentPageReport\"\r\n            :rowsPerPageOptions=\"[25,50,100]\" responsiveLayout=\"scroll\"\r\n            currentPageReportTemplate=\"Showing {first} to {last} of total {totalRecords} users\"\r\n            :globalFilterFields=\"['name','surname','email']\"\r\n            selectionMode=\"single\"\r\n            @rowSelect=\"onRowSelect\"\r\n            @rowClick=\"onRowClick\"\r\n            \r\n                        :loading=\"loading1\"\r\n            scrollable \r\n          \r\n            >\r\n   \r\n\r\n\r\n              <template #header >\r\n                    \r\n                    <div style=\"display:none\" class=\"flex justify-content-between\">\r\n                        <!-- <Button type=\"button\" icon=\"pi pi-filter-slash\" label=\"Clear\" class=\"p-button-outlined\" @click=\"clearFilter1()\"/>  -->\r\n                        <span class=\"p-input-icon-left\" id=\"searchBox\">\r\n                            <i class=\"pi pi-search\" />\r\n                            <InputText v-model=\"filters['global'].value\" placeholder=\"Keyword Search\" />\r\n                        </span>\r\n                    </div>\r\n\r\n                    <div class=\"row\">\r\n                      <div class=\"col-md-4\">\r\n                        <Button style=\"min-width:120px\" v-tooltip.bottom=\"{ value: 'New trip', showDelay: 1000, hideDelay: 300 }\" type=\"button\" icon=\"pi pi-pencil\" class=\"p-button-text\" id=\"newTripButton\" @click=\"newTrip\" label=\"New trip\"/>\r\n                        &nbsp;\r\n                        <Button style=\"min-width:120px\" v-tooltip.bottom=\"{ value: 'Download report', showDelay: 1000, hideDelay: 300 }\" type=\"button\" icon=\"pi pi-download\" class=\"p-button-text\" id=\"newTripButton\" @click=\"downloadReport\" label=\"Download Report\"/>\r\n                      </div>\r\n                        \r\n                      <div class=\"col-md-8 text-end\">\r\n                           <!-- something more ont right side table header-->\r\n                        </div>\r\n                    </div>\r\n\r\n\r\n                   \r\n\r\n            </template>\r\n           \r\n           <template #empty>\r\n                    \r\n           </template>\r\n           <template #loading>\r\n                    Loading data. Please wait...\r\n           </template>\r\n\r\n\r\n      <Column  field=\"isActive\" header=\"Active\" dataType=\"boolean\" style=\"max-width:75px;\" bodyStyle=\"width:40px;text-align:center\">\r\n        \r\n        <template #body=\"{ data }\">\r\n           <div class=\"text-center;width:100%\">\r\n                <i v-if=\"data.isActive\" class=\"pi pi-check\"></i>\r\n                <i v-if=\"!data.isActive\" class=\"pi pi-times\"></i>\r\n           </div>\r\n           <span style=\"display: none;\" class=\"hiddenFields\" :data=data.id></span> <!-- this is only that I have something to indetify which row is clicked :) -->\r\n        </template>\r\n        \r\n      \r\n    </Column>      \r\n\r\n    <Column field=\"name\" header=\"Name\" :sortable=\"true\" bodyStyle=\"text-align: left\"  style=\"max-width:250px;\">\r\n        <template #filter=\"{filterModel}\">\r\n            <InputText  type=\"text\" v-model=\"filterModel.value\" class=\"p-column-filter\" :placeholder=\"`Name`\"/>\r\n        </template>\r\n    </Column>\r\n\r\n    <Column field=\"surname\" header=\"Surname\" :sortable=\"true\" bodyStyle=\"text-align: left\"  style=\"max-width:250px;\">\r\n        <template #filter=\"{filterModel}\">\r\n            <InputText  type=\"text\" v-model=\"filterModel.value\" class=\"p-column-filter\" :placeholder=\"`Surname`\"/>\r\n        </template>\r\n    </Column>\r\n\r\n    <Column field=\"email\" header=\"Email\" :sortable=\"true\" bodyStyle=\"text-align: left\"  style=\"max-width:250px;\"> \r\n        <template #filter=\"{filterModel}\">\r\n            <InputText  type=\"text\" v-model=\"filterModel.value\" class=\"p-column-filter\" :placeholder=\"`Email`\"/>\r\n        </template>\r\n    </Column>\r\n\r\n\r\n    <Column>\r\n    </Column>\r\n\r\n\r\n    <Column  v-if=\"userType=='ADMIN'\" style=\"text-align: right;\" class=\"actionsColumns\">\r\n        <template #header >\r\n          <div style=\"right: 20px;  position: absolute;\">\r\n            <div style=\"float:right\">Actions</div>\r\n          </div>\r\n        </template>\r\n        <template #body>\r\n          \r\n           \r\n            <div class=\"fa-stack fa-2x actionIcons\" style=\"font-size:15px\"  v-tooltip.bottom=\"{ value: 'Edit user', showDelay: 1000, hideDelay: 300 }\"  @click=\"editClick\">\r\n              <i class=\"fa-solid fa-square fa-stack-2x\"></i>\r\n              <i class=\"fa-stack-1x fa-solid fa-pen-to-square\" style=\"color: white\"></i>\r\n            </div>\r\n            \r\n           \r\n            <div class=\"fa-stack fa-2x actionIcons\" style=\"font-size:15px;color:#aa0000\" v-tooltip.left=\"{ value: 'Delete user', showDelay: 1000, hideDelay: 300 }\" @click=\"deleteClick\">\r\n              <i class=\"fa-solid fa-square fa-stack-2x\"></i>\r\n              <i class=\"fa-stack-1x fa-solid fa-trash-can\" style=\"color: white\"></i>\r\n            </div>\r\n        \r\n        </template>\r\n\r\n    </Column>\r\n\r\n\r\n\r\n\r\n    \r\n\r\n\r\n    \r\n   \r\n\r\n\r\n    <template #paginatorstart>\r\n    \r\n    </template>         \r\n    <template #paginatorend>\r\n      \r\n    </template>  \r\n    \r\n\r\n   \r\n\r\n\r\n</DataTable>\r\n</div>\r\n\r\n\r\n</template>\r\n\r\n\r\n<script>\r\n// Primevue datatable help: https://www.primefaces.org/primevue/datatable //\r\n\r\nimport DataTable from \"primevue/datatable\";\r\nimport Column from \"primevue/column\";\r\nimport InputText from 'primevue/inputtext';\r\n//import Checkbox from 'primevue/checkbox';\r\n\r\n\r\n//import Button from 'primevue/button';\r\n//import Dialog from 'primevue/dialog';\r\n//import InlineMessage from 'primevue/inlinemessage';\r\n//import InputNumber from 'primevue/inputnumber';\r\n//import TabView from 'primevue/tabview';\r\n//import TabPanel from 'primevue/tabpanel';\r\n//import Dropdown from 'primevue/dropdown';\r\n\r\nimport axios from \"axios\";\r\nimport $ from \"jquery\";\r\n\r\nimport {FilterMatchMode,FilterOperator} from 'primevue/api';         // for filtering\r\n\r\n\r\n\r\nexport default {\r\n    props: [\"systemId\"],\r\n    components: {\r\n        DataTable,\r\n        Column,\r\n        InputText,\r\n        //Checkbox,\r\n        //Button,\r\n        //Dialog,\r\n        //InlineMessage,\r\n        //InputNumber,\r\n        //TabView,\r\n        //TabPanel,\r\n        //Dropdown,\r\n    \r\n  },\r\n  \r\n  data() {\r\n        return {\r\n            userType : '',\r\n            isLoading:false,\r\n            tableHeight:'calc( 100vh - 215px)',\r\n            allUsers:[],\r\n\r\n            editUserName:'',\r\n            editUserSurname:'',\r\n            editUserEmail:'',\r\n            editUserType:'',\r\n            editUserActive:false,\r\n           \r\n            filters: {\r\n               'global': {value: null, matchMode: FilterMatchMode.CONTAINS},\r\n               'name':{operator: FilterOperator.AND, constraints: [{value: null, matchMode: FilterMatchMode.CONTAINS}]}, \r\n               'surname': {operator: FilterOperator.AND, constraints: [{value: null, matchMode: FilterMatchMode.CONTAINS}]}, \r\n               'email': {operator: FilterOperator.AND, constraints: [{value: null, matchMode: FilterMatchMode.CONTAINS}]},\r\n               'isActive': {operator: FilterOperator.AND, constraints: [{value: null, matchMode: FilterMatchMode.CONTAINS}]},\r\n              \r\n            },\r\n            matchModes: [\r\n                    {label: 'Starts With', value: FilterMatchMode.STARTS_WITH},\r\n                    {label: 'Contains', value: FilterMatchMode.CONTAINS},\r\n             ],\r\n        }\r\n    },\r\n    mounted() {\r\n        this.userType = (this.$store.getters.getUserType).toUpperCase();\r\n        this.getAllUsers();\r\n        this.moveSearchBoxToTop();\r\n    },\r\n    methods: {\r\n\r\n     reorganizePagination(){\r\n      $(\".p-paginator-current\").detach().appendTo('.p-paginator-left-content');\r\n      $(\".p-paginator-rpp-options\").detach().appendTo('.p-paginator-right-content');\r\n    },\r\n    \r\n    moveSearchBoxToTop(){\r\n        //default search box is inside header of the datatable, must move to the top of the page\r\n        $('#hereMoveSearch').empty();\r\n        $(\"#searchBox\").detach().appendTo('#hereMoveSearch');\r\n    },\r\n\r\n    onRowClick(event) {\r\n        \r\n        // alert( event.data.id);\r\n            this.ClickedRowId = event.data.id; // key for update\r\n          \r\n            this.editUserName = event.data.name;\r\n            this.editUserSurname = event.data.surname;\r\n            this.editUserEmail = event.data.email;\r\n            this.editUserType = event.data.type;\r\n            this.editUserActive = event.data.isActive;\r\n          \r\n  \r\n          \r\n            $('.hiddenFields').each(function() {\r\n              \r\n              $(this).closest('tr').removeClass('highlight');\r\n             });\r\n            \r\n            $('.hiddenFields').each(function() {\r\n             \r\n              if($(this).attr('data')==event.data.id){\r\n                $(this).closest('tr').addClass('highlight');\r\n               }\r\n            \r\n            });\r\n  \r\n        },\r\n\r\n        onRowSelect(event) {\r\n            //here open panel, but for manage users don't need to open panel\r\n        },\r\n\r\n        getAllUsers(){\r\n          this.isLoading = true; \r\n\r\n          const sessionId=localStorage.getItem(\"token\");\r\n          const baseUrl = localStorage.getItem(\"_rootRestUrl\");\r\n          \r\n          let formData = JSON.stringify({ sessionId: sessionId });\r\n          let config = {\r\n            headers: {\r\n              \"Content-Type\": \"application/json\",\r\n            },\r\n\r\n          };\r\n\r\n           axios\r\n            .get(baseUrl + \"/api/v1/User/Users/?sessionId=\"+sessionId+\"\", formData, config)\r\n            .then((response) => {\r\n               this.allUsers = response.data;\r\n               this.reorganizePagination();\r\n               this.isLoading = false; \r\n            })           \r\n             .catch(function (error) {\r\n                // handle error\r\n               \r\n          \r\n                if(error.response.status=='401') { //not authorized, token expires\r\n                //hm, I don't know why this three next lines not work :( so I made it \"manual logout\"\r\n             \r\n               // this.isLoading = false;  \r\n               // this.$store.dispatch(\"logout\");\r\n               // this.$router.replace(\"/\");\r\n\r\n                localStorage.removeItem(\"token\");\r\n                localStorage.removeItem(\"userId\");\r\n                localStorage.removeItem(\"userFullname\");\r\n                localStorage.removeItem(\"userType\");\r\n                localStorage.removeItem(\"tokenExpiration\");\r\n                localStorage.removeItem(\"expiresIn\");\r\n               \r\n\r\n                document.location = '/';\r\n\r\n                this.isLoading = false; \r\n                }\r\n\r\n             });\r\n\r\n\r\n        }\r\n\r\n    },\r\n\r\n\r\n}\r\n\r\n\r\n\r\n</script>\r\n\r\n\r\n<style>\r\n@import \"datatables.net-dt\";\r\n@import \"primevue/resources/themes/bootstrap4-light-blue/theme.css\"; /*theme*/\r\n@import \"primevue/resources/primevue.min.css   \"; /*core css*/\r\n@import \"primeicons/primeicons.css 