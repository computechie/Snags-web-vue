{"ast":null,"code":"// Primevue datatable help: https://www.primefaces.org/primevue/datatable //\nimport DataTable from \"primevue/datatable\";\nimport Column from \"primevue/column\";\nimport InputText from 'primevue/inputtext'; //import Button from 'primevue/button';\n//import Dialog from 'primevue/dialog';\n//import InlineMessage from 'primevue/inlinemessage';\n//import InputNumber from 'primevue/inputnumber';\n//import TabView from 'primevue/tabview';\n//import TabPanel from 'primevue/tabpanel';\n//import Dropdown from 'primevue/dropdown';\n\nimport axios from \"axios\"; //import $ from \"jquery\";\n\nimport { FilterMatchMode, FilterOperator } from 'primevue/api'; // for filtering\n\nexport default {\n  props: [\"systemId\"],\n  components: {\n    DataTable,\n    Column,\n    InputText //Button,\n    //Dialog,\n    //InlineMessage,\n    //InputNumber,\n    //TabView,\n    //TabPanel,\n    //Dropdown,\n\n  },\n\n  data() {\n    return {\n      tableHeight: 'calc( 100vh - 215px)',\n      allUsers: [],\n      filters: {\n        'global': {\n          value: null,\n          matchMode: FilterMatchMode.CONTAINS\n        },\n        'name': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'surname': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'email': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        }\n      },\n      matchModes: [{\n        label: 'Starts With',\n        value: FilterMatchMode.STARTS_WITH\n      }, {\n        label: 'Contains',\n        value: FilterMatchMode.CONTAINS\n      }]\n    };\n  },\n\n  mounted() {\n    this.getAllUsers();\n  },\n\n  methods: {\n    getAllUsers() {\n      const sessionId = localStorage.getItem(\"token\");\n      const baseUrl = localStorage.getItem(\"_rootRestUrl\");\n      let formData = JSON.stringify({\n        sessionId: sessionId\n      });\n      let config = {\n        headers: {\n          \"Content-Type\": \"application/json\"\n        }\n      };\n      axios.get(baseUrl + \"/api/v1/User/Users/?sessionId=\" + sessionId + \"\", formData, config).then(response => {\n        this.allUsers = response.data;\n      }).catch(function (error) {\n        // handle error\n        console.table(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4FA;AAEA,OAAOA,SAAP,MAAsB,oBAAtB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AACA,OAAOC,SAAP,MAAsB,oBAAtB,C,CACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA,OAAOC,KAAP,MAAkB,OAAlB,C,CACA;;AAEA,SAAQC,eAAR,EAAwBC,cAAxB,QAA6C,cAA7C,C,CAAqE;;AAIrE,eAAe;EACXC,KAAK,EAAE,CAAC,UAAD,CADI;EAEXC,UAAU,EAAE;IACRP,SADQ;IAERC,MAFQ;IAGRC,SAHQ,CAIR;IACA;IACA;IACA;IACA;IACA;IACA;;EAVQ,CAFD;;EAgBbM,IAAI,GAAG;IACD,OAAO;MACHC,WAAW,EAAC,sBADT;MAEHC,QAAQ,EAAC,EAFN;MAIHC,OAAO,EAAE;QACN,UAAU;UAACC,KAAK,EAAE,IAAR;UAAcC,SAAS,EAAET,eAAe,CAACU;QAAzC,CADJ;QAEN,QAAO;UAACC,QAAQ,EAAEV,cAAc,CAACW,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAET,eAAe,CAACU;UAAzC,CAAD;QAA5C,CAFD;QAGN,WAAW;UAACC,QAAQ,EAAEV,cAAc,CAACW,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAET,eAAe,CAACU;UAAzC,CAAD;QAA5C,CAHL;QAIN,SAAS;UAACC,QAAQ,EAAEV,cAAc,CAACW,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAET,eAAe,CAACU;UAAzC,CAAD;QAA5C;MAJH,CAJN;MAWHI,UAAU,EAAE,CACJ;QAACC,KAAK,EAAE,aAAR;QAAuBP,KAAK,EAAER,eAAe,CAACgB;MAA9C,CADI,EAEJ;QAACD,KAAK,EAAE,UAAR;QAAoBP,KAAK,EAAER,eAAe,CAACU;MAA3C,CAFI;IAXT,CAAP;EAgBH,CAjCU;;EAkCXO,OAAO,GAAG;IACN,KAAKC,WAAL;EAEH,CArCU;;EAsCXC,OAAO,EAAE;IACLD,WAAW,GAAE;MAEX,MAAME,SAAS,GAACC,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAhB;MACA,MAAMC,OAAM,GAAIF,YAAY,CAACC,OAAb,CAAqB,cAArB,CAAhB;MAEA,IAAIE,QAAO,GAAIC,IAAI,CAACC,SAAL,CAAe;QAAEN,SAAS,EAAEA;MAAb,CAAf,CAAf;MACA,I