this.timestamp;\n        //  $('#'+imageId).attr('src',newPhotoUrl)\n\n        setTimeout(() => this.$emit('showloader', false), 375);\n      }).catch(function (error) {\n        // handle error\n        console.table(error); //   this.$emit('showloader',false);\n      });\n    },\n\n    sendBackIdForDelete(imageId) {\n      this.displayDeletePhotoConfirm = true;\n      this.photoIdForDelete = imageId;\n      this.$emit('delete-photo', imageId);\n    },\n\n    sendBackIdForDeleteModal(imageId) {\n      this.displayDeletePhotoConfirm = true;\n      this.photoIdForDelete = imageId;\n    },\n\n    deletePhoto(photoId) {\n      this.$emit('delete-photomodal', photoId);\n      this.visibleImagesModal = false;\n      this.displayDeletePhotoConfirm = false;\n    },\n\n    onSlideChange() {\n      var imageId = $('.zoomedImageHolder .swiper .swiper-wrapper .swiper-slide-active img').attr('id');\n      this.zoomedImageId = imageId;\n    }\n\n  },\n\n  mounted() {\n    // alert(this.tripImages)\n    alert(parent.snagDetailsData.notes);\n  }\n\n};","map":{"version":3,"mappings":"AAgIE;AACA,SAASA,UAAT,EAAqBC,UAArB,EAAiCC,SAAjC,EAA4CC,IAA5C,QAAwD,gBAAxD;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AACA,SAASC,MAAT,QAAuB,gBAAvB,C,CAEA;;AACA,SAASC,MAAT,EAAiBC,WAAjB,QAAoC,YAApC;AAEA,OAAO,YAAP;AACA,OAAO,uBAAP;AACA,OAAO,sBAAP;AACA,OAAOC,MAAP,MAAmB,iBAAnB,C,CACD;;AAGA,OAAOC,KAAP,MAAkB,OAAlB;AACC,OAAOC,CAAP,MAAc,QAAd,C,CAGC;;AACD,eAAe;EACbC,KAAK,EAAC,CAAC,YAAD,EAAc,iBAAd,EAAgC,UAAhC,CADO;EAEbC,UAAU,EAAE;IACVN,MADU;IAEVC,WAFU;IAGVC,MAHU;IAIVJ;EAJU,CAFC;EASbS,KAAK,EAAC,CAAC,YAAD,EAAe,QAAf,EAAyB,aAAzB,EAAwC,kBAAxC,CATO;;EAUbC,KAAK,GAAG;IACN,MAAMC,QAAO,GAAI,MAAM,CACtB;IAGA,CAJD;;IAOA,OAAO;MACLA,QADK;MACIR,WADJ;MACgBF,MADhB;MAGLW,OAAO,EAAE,CAAChB,UAAD,EAAaC,UAAb,EAAyBC,SAAzB,EAAoCC,IAApC;IAHJ,CAAP;EAKD,CAvBY;;EAyBbc,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAC,EADR;MAELC,SAAQ,EAAIC,IAAI,CAACC,GAAL,EAFP;MAGLC,MAAM,EAAC,EAHF;MAILC,cAAa,EAAI,CAJZ;MAKLC,yBAAyB,EAAC,KALrB;MAMLC,aAAa,EAAE,EANV;MAOLC,gBAAgB,EAAC,EAPZ;MAQLC