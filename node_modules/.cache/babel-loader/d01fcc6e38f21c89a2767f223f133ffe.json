lass(\"hiddenArrow\"); ///1st level\n\n    const object = this.items;\n\n    for (var key1A in object) {\n      for (var key2A in object[key1A]) {\n        if (key2A == \"id\") {\n          var rememberId1 = object[key1A][key2A];\n          $('#folder' + rememberId1).css(\"color\", object[key1A]['color']);\n        }\n\n        if (key2A == \"children\") {\n          $(\"#arrow\" + rememberId1).removeClass(\"hiddenArrow\"); ///2nd level\n\n          var object2 = object[key1A][\"children\"];\n\n          for (var key1B in object2) {\n            for (var key2B in object2[key1B]) {\n              if (key2B == \"id\") {\n                var rememberId2 = object2[key1B][key2B];\n                $('#folder' + rememberId2).css(\"color\", object2[key1B]['color']);\n              }\n\n              if (key2B == \"children\") {\n                $(\"#arrow\" + rememberId2).removeClass(\"hiddenArrow\"); ///3rd level\n\n                var object3 = object2[key1B][\"children\"];\n\n                for (var key1C in object3) {\n                  for (var key2C in object3[key1C]) {\n                    if (key2C == \"id\") {\n                      var rememberId3 = object3[key1C][key2C];\n                      $('#folder' + rememberId3).css(\"color\", object3[key1C]['color']);\n                    }\n\n                    if (key2C == \"children\") {\n                      $(\"#arrow\" + rememberId3).removeClass(\"hiddenArrow\"); ///4th level\n\n                      var object4 = object3[key1C][\"children\"];\n\n                      for (var key1D in object4) {\n                        for (var key2D in object4[key1D]) {\n                          if (key2D == \"id\") {\n                            var rememberId4 = object4[key1D][key2D];\n                            $('#folder' + rememberId4).css(\"color\", object4[key1D]['color']);\n                          }\n\n                          if (key2D == \"children\") {\n                            $(\"#arrow\" + rememberId4).removeClass(\"hiddenArrow\"); ///5th level\n\n                            var object5 = object4[key1D][\"children\"];\n\n                            for (var key1E in object5) {\n                              for (var key2E in object5[key1E]) {\n                                if (key2E == \"id\") {\n                                  var rememberId5 = object5[key1E][key2E];\n                                  $('#folder' + rememberId5).css(\"color\", object5[key1E]['color']);\n                                }\n\n                                if (key2E == \"children\") {\n                                  $(\"#arrow\" + rememberId5).removeClass(\"hiddenArrow\");\n                                }\n                              }\n                            } ////end 5th level\n\n                          }\n                        }\n                      } ////end 4th level\n\n                    }\n                  }\n                } ////end 3rd level\n\n              }\n            }\n          } ////end 2nd level\n\n        }\n      }\n    } ////end 1nd level\n    /// WHat its Default open ? (click on item:)\n    //  this.clickThis(\"treeA-1\", \"subLiA\", 1);\n    //------------------\n\n  }\n\n};","map":{"version":3,"mappings":"AA8HA,OAAOA,CAAP,MAAc,QAAd;AAEA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,EADT;MAELC,MAAM,EAAE,IAFH;MAGLC,SAAS,EAAE,KAHN;MAKLC,KAAK,EAAE,CACL;QACEC,IAAI,EAAE,eADR;QAEEC,EAAE,EAAE,CAFN;QAGEC,KAAK,EAAE,MAHT;QAIEC,QAAQ,EAAE,CACR;UACEH,IAAI,EAAE,gBADR;UAEEC,EAAE,EAAE,CAFN;UAGEC,KAAK,EAAE;QAHT,CADQ,EAMR;UACEF,IAAI,EAAE,yBADR;UAEEC,EAAE,EAAE,CAFN;UAGEC,KAAK,EAAE;QAHT,CANQ,EAWR;UACEF,IAAI,EAAE,yBADR;UAEEC,EAAE,EAAE,CAFN;UAGEC,KAAK,EAAE,OAHT;UAIEC,QAAQ,EAAE,CACR;YACEH,IAAI,EAAE,yBADR;YAEEC,EAAE,EAAE,CAFN;YAGEC,KAAK,EAAE;UAHT,CADQ,EAMR;YACEF,IAAI,EAAE,yBADR;YAGEC,EAAE,EAAE,CAHN;YAIEC,KAAK,EAAE,SAJT;YAMEC,QAAQ,EAAE,CACR;cACEH,IAAI,EAAE,2BADR;cAEEC,EAAE,EAAE,CAFN;cAGEC,KAAK,EAAE,SAHT;cAKLC,QAAQ,EAAE,CACT;gBACEH,IAAI,EAAE,8BADR;gBAEEC,EAAE