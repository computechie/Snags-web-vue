ntKey);\n    },\n\n    openPanel(documentKey) {\n      console.log(documentKey);\n    },\n\n    initFilters1() {\n      this.filters1 = {\n        'global': {\n          value: null,\n          matchMode: FilterMatchMode.CONTAINS\n        }\n      };\n    },\n\n    clearFilter1() {\n      this.initFilters1();\n    },\n\n    getFiles() {\n      /*  $(\"#datatable\").DataTable({\r\n              lengthMenu: [\r\n                [25, 50, 100, -1],\r\n                [25, 50, 100, \"All\"],\r\n              ],\r\n              pageLength: 25,\r\n            });*/\n    },\n\n    format_date(value, pattern) {\n      if (value) {\n        return moment(String(value)).format(pattern);\n      }\n    }\n\n  },\n\n  mounted() {\n    this.getTrips();\n  },\n\n  created() {\n    this.aSystemDoc = localStorage.getItem(\"docType\");\n    this.initFilters1();\n    this.loading1 = false;\n  }\n\n};","map":{"version":3,"mappings":"AAuIA;AAEA,OAAOA,SAAP,MAAsB,oBAAtB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AACA,OAAOC,SAAP,MAAsB,oBAAtB;AACA,OAAOC,MAAP,MAAmB,iBAAnB,C,CACA;AACA;;AAEA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,SAAQC,eAAR,EAAwBC,cAAxB,QAA6C,cAA7C,C,CAAqE;;AAErE,OAAOC,MAAP,MAAmB,QAAnB;AAEA,eAAe;EACbC,KAAK,EAAE,CAAC,UAAD,CADM;EAEbC,UAAU,EAAE;IACVP,SADU;IACAC,MADA;IAEVH,SAFU;IAGVC;EAHU,CAFC;;EAObS,IAAI,GAAG;IACL,OAAO;MACDC,QAAQ,EAAE,IADT;MAEDC,QAAQ,EAAE,IAFT;MAGDC,QAAQ,EAAE,IAHT;MAIDC,OAAO,EAAE;QACJ,UAAU;UAACC,KAAK,EAAE,IAAR;UAAcC,SAAS,EAAEX,eAAe,CAACY;QAAzC,CADN;QAEJ,gBAAe;UAACC,QAAQ,EAAEZ,cAAc,CAACa,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAEX,eAAe,CAACY;UAAzC,CAAD;QAA5C,CAFX;QAGJ,QAAQ;UAACC,QAAQ,EAAEZ,cAAc,CAACa,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAEX,eAAe,CAACY;UAAzC,CAAD;QAA5C,CAHJ;QAIJ,MAAM;UAACC,QAAQ,EAAEZ,cAAc,CAACa,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAEX,eAAe,CAACY;UAAzC,CAAD;QAA5C,CAJF;QAKJ,eAAe;UAACC,QAAQ,EAAEZ,cAAc,CAACa,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAEX,eAAe,CAACY;UAAzC,CAAD;QAA5C,CALX;