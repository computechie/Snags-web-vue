ator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'sumVAT': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'comments': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'date': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        }\n      },\n      aSystemDoc: \"\",\n      matchModes: [{\n        label: 'Starts With',\n        value: FilterMatchMode.STARTS_WITH\n      }, {\n        label: 'Contains',\n        value: FilterMatchMode.CONTAINS\n      }],\n      allTrips: [],\n      fromDate: '',\n      toDate: ''\n    };\n  },\n\n  methods: {\n    moveSearchBoxToTop() {\n      $(\"#searchBox\").detach().appendTo('#hereMoveSearch');\n    },\n\n    rowClick() {//   alert(\"row click\")\n    },\n\n    formatDate() {},\n\n    getTrips() {\n      if (this.fromDate == '') {\n        this.fromDate = '23/10/2023';\n      }\n\n      if (this.toDate == '') {\n        this.toDate = '30/10/2023';\n      }\n\n      const fromDateFull = '2023-10-23 00:00:00';\n      const toDateFull = '2023-10-30 23:59:00';\n      const sessionId = localStorage.getItem(\"token\");\n      const DateFrom = fromDateFull;\n      const DateTo = toDateFull;\n      const UserId = localStorage.getItem(\"userId\");\n      const UserType = 'ADMIN';\n      const baseUrl = localStorage.getItem(\"_rootRestUrl\");\n      let formData = JSON.stringify({\n        sessionId: sessionId,\n        DateFrom: DateFrom,\n        DateTo: DateTo,\n        UserId: UserId,\n        UserType: UserType\n      });\n      let config = {\n        headers: {\n          \"Content-Type\": \"application/json\"\n        }\n      };\n      axios.get(baseUrl + \"/api/v1/Trip/Trips/?sessionId=\" + sessionId + \"&DateFrom=\" + DateFrom + \"&DateTo=\" + DateTo + \"&UserId=\" + UserId + \"&UserType=\" + UserType + \"\", formData, config).then(response => {\n        console.table(response.data);\n        this.allTrips = response.data;\n      }).catch(function (error) {\n        // handle error\n        console.log(error);\n      });\n    },\n\n    onRowSelect(event) {\n      this.getDocumentDetails(event.data.id);\n    },\n\n    onRowUnselect(event) {\n      console.log(event);\n    },\n\n    getDocumentDetails(documentKey) {\n      //load from api them\n      this.openPanel(documentKey);\n    },\n\n    openPanel(documentKey) {\n      alert(documentKey);\n    },\n\n    initFilters1() {\n      this.filters1 = {\n        'global': {\n          value: null,\n          matchMode: FilterMatchMode.CONTAINS\n        },\n        'userFullName': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'from': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'to': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'companyName': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'cargoName': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'distance': {\n          operator: FilterOperator.AND,\n      