{"ast":null,"code":"// Primevue datatable help: https://www.primefaces.org/primevue/datatable //\nimport DataTable from \"primevue/datatable\";\nimport Column from \"primevue/column\";\nimport InputText from 'primevue/inputtext';\nimport Button from 'primevue/button';\nimport Calendar from 'primevue/calendar'; //import ColumnGroup from 'primevue/columngroup';     //optional for column grouping\n//import Row from 'primevue/row';                     //optional for row\n\nimport axios from \"axios\";\nimport $ from \"jquery\";\nimport { FilterMatchMode, FilterOperator } from 'primevue/api'; // for filtering\n\nimport moment from \"moment\";\nexport default {\n  props: [\"systemId\"],\n  components: {\n    InputText,\n    Button,\n    DataTable,\n    Column,\n    Calendar\n  },\n\n  data() {\n    return {\n      isLoading: true,\n      userType: '',\n      loading1: true,\n      loading2: true,\n      filters1: null,\n      panelWidth: '0px',\n      filters: {\n        'global': {\n          value: null,\n          matchMode: FilterMatchMode.CONTAINS\n        },\n        'userFullName': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'from': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'to': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'companyName': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'cargoName': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'distance': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'sumVAT': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'comments': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'date': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        }\n      },\n      aSystemDoc: \"\",\n      matchModes: [{\n        label: 'Starts With',\n        value: FilterMatchMode.STARTS_WITH\n      }, {\n        label: 'Contains',\n        value: FilterMatchMode.CONTAINS\n      }],\n      allTrips: [],\n      fromDate: '',\n      toDate: ''\n    };\n  },\n\n  methods: {\n    reorganizePagination() {\n      $(\".p-paginator-current\").detach().appendTo('.p-paginator-left-content');\n      $(\".p-paginator-rpp-options\").detach().appendTo('.p-paginator-right-content');\n    },\n\n    moveSearchBoxToTop() {\n      $(\"#searchBox\").detach().appendTo('#hereMoveSearch');\n    },\n\n    newTrip() {\n      alert(\"New trip clicked\");\n    },\n\n    editClick() {\n      alert(\"Edit clicked\");\n    },\n\n    deleteClick() {\n      alert(\"Delewte clicked\");\n    },\n\n    rowClick() {//   alert(\"row click\")\n    },\n\n    refreshData() {\n      if (this.fromDate.length != 10) {\n        this.fromDate = this.format_date(this.fromDate, 'DD/MM/YYYY');\n      }\n\n      if (this.toDate.length != 10) {\n        this.toDate = this.format_date(this.toDate, 'DD/MM/YYYY');\n      }\n\n      this.getTrips();\n    },\n\n    format_date_to_full(aDate, aTime) {\n      //excpected input: 14/04/2023\n      var year = aDate.substring(6, 10);\n      var month = aDate.substring(3, 5);\n      var day = aDate.substring(0, 2);\n      return year + '-' + month + '-' + day + \" \" + aTime;\n    },\n\n    getTrips() {\n      this.isLoading = true;\n      this.panelWidth = '0px';\n      const fromDateFull = this.format_date_to_full(this.fromDate, '00:00:00');\n      const toDateFull = this.format_date_to_full(this.toDate, '23:59:59');\n      const sessionId = localStorage.getItem(\"token\");\n      const DateFrom = fromDateFull;\n      const DateTo = toDateFull;\n      const UserId = localStorage.getItem(\"userId\");\n      const UserType = this.userType;\n      const baseUrl = localStorage.getItem(\"_rootRestUrl\");\n      let formData = JSON.stringify({\n        sessionId: sessionId,\n        DateFrom: DateFrom,\n        DateTo: DateTo,\n        UserId: UserId,\n        UserType: UserType\n      });\n      let config = {\n        headers: {\n          \"Content-Type\": \"application/json\"\n        }\n      };\n      axios.get(baseUrl + \"/api/v1/Trip/Trips/?sessionId=\" + sessionId + \"&DateFrom=\" + DateFrom + \"&DateTo=\" + DateTo + \"&UserId=\" + UserId + \"&UserType=\" + UserType + \"\", formData, config).then(response => {\n        console.table(response.data);\n        this.allTrips = response.data;\n        this.isLoading = false;\n        this.reorganizePagination();\n      }).catch(function (error) {\n        // handle error\n        console.log(error);\n        this.isLoading = false;\n        this.$store.dispatch(\"logout\");\n        this.$router.push(\"/\");\n      });\n    },\n\n    onRowSelect(event) {\n      if (event.originalEvent.target.tagName != 'I') {\n        //if not clicked on I element (icon ) then open panel\n        this.getDocumentDetails(event.data.id);\n      }\n    },\n\n    getDocumentDetails(documentKey) {\n      //load from api them\n      this.openPanel(documentKey);\n    },\n\n    openPanel(documentKey) {\n      this.panelWidth = '750px';\n      alert(documentKey);\n    },\n\n    initFilters1() {\n      this.filters1 = {\n        'global': {\n          value: null,\n          matchMode: FilterMatchMode.CONTAINS\n        },\n        'userFullName': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'from': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'to': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'companyName': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'cargoName': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'distance': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'sumVAT': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        },\n        'comments': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.CONTAINS\n          }]\n        }\n      };\n    },\n\n    clearFilter1() {\n      this.initFilters1();\n    },\n\n    getFiles() {\n      /*  $(\"#datatable\").DataTable({\r\n              lengthMenu: [\r\n                [25, 50, 100, -1],\r\n                [25, 50, 100, \"All\"],\r\n              ],\r\n              pageLength: 25,\r\n            });*/\n    },\n\n    format_date(value, pattern) {\n      if (value) {\n        return moment(String(value)).format(pattern);\n      }\n    }\n\n  },\n\n  mounted() {\n    /*this.fromDate = \"15/10/2023\";\r\n    this.toDate   = \"15/10/2023\";*/\n    this.fromDate = this.format_date(moment().startOf('week').add(1, 'days'), 'DD/MM/YYYY');\n    this.toDate = this.format_date(moment().endOf('week').add(1, 'days'), 'DD/MM/YYYY');\n    this.$emit(\"showSearchbar\", true);\n    this.userType = this.$store.getters.getUserType.toUpperCase();\n    this.getTrips();\n    this.moveSearchBoxToTop();\n  },\n\n  created() {\n    this.aSystemDoc = localStorage.getItem(\"docType\");\n    this.initFilters1();\n    this.loading1 = false;\n  }\n\n};","map":{"version":3,"mappings":"AA4MA;AAEA,OAAOA,SAAP,MAAsB,oBAAtB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AACA,OAAOC,SAAP,MAAsB,oBAAtB;AACA,OAAOC,MAAP,MAAmB,iBAAnB;AACA,OAAOC,QAAP,MAAqB,mBAArB,C,CAEA;AACA;;AAEA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,CAAP,MAAc,QAAd;AAEA,SAAQC,eAAR,EAAwBC,cAAxB,QAA6C,cAA7C,C,CAAqE;;AAErE,OAAOC,MAAP,MAAmB,QAAnB;AAIA,eAAe;EACbC,KAAK,EAAE,CAAC,UAAD,CADM;EAEbC,UAAU,EAAE;IACVT,SADU;IACAC,MADA;IAEVH,SAFU;IAGVC,MAHU;IAIVG;EAJU,CAFC;;EASbQ,IAAI,GAAG;IACL,OAAO;MACDC,SAAS,EAAE,IADV;MAEDC,QAAO,EAAI,EAFV;MAGDC,QAAQ,EAAE,IAHT;MAIDC,QAAQ,EAAE,IAJT;MAKDC,QAAQ,EAAE,IALT;MAMDC,UAAU,EAAC,KANV;MAODC,OAAO,EAAE;QACJ,UAAU;UAACC,KAAK,EAAE,IAAR;UAAcC,SAAS,EAAEd,eAAe,CAACe;QAAzC,CADN;QAEJ,gBAAe;UAACC,QAAQ,EAAEf,cAAc,CAACgB,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAEd,eAAe,CAACe;UAAzC,CAAD;QAA5C,CAFX;QAGJ,QAAQ;UAACC,QAAQ,EAAEf,cAAc,CAACgB,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAEd,eAAe,CAACe;UAAzC,CAAD;QAA5C,CAHJ;QAIJ,MAAM;UAACC,QAAQ,EAAEf,cAAc,CAACgB,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAAK,EAAE,IAAR;YAAcC,SAAS,EAAEd,eAAe,CAACe;UAAzC,CAAD;QAA5C,CAJF;QAKJ,eAAe;UAACC,QAAQ,EAAEf,cAAc,CAACgB,GAA1B;UAA+BC,WAAW,EAAE,CAAC;YAACL,KAA