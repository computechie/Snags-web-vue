 <div class=\"col-12\">\r\n        <div\r\n          class=\"wrapper2\"\r\n          @mouseup=\"endDragging2()\"\r\n          style=\"position: relative\"\r\n        >\r\n          <div\r\n            class=\"systemsTree\"\r\n            :style=\"{\r\n              \r\n              width: `${treeWidth}px`,\r\n            }\"\r\n          >\r\n          <div id=\"hhh\" style=\"overflow:auto !important\">\r\n            <SystemTree @atfolder-click=\"folderClick\" />\r\n          </div>\r\n          </div>\r\n          <div\r\n            class=\"divider2\"\r\n            :style=\"{ left: `${dividerPosition2}px` }\"\r\n            @mousedown=\"startDragging2()\"\r\n          ></div>\r\n          <div\r\n            class=\"mainContent\"\r\n            :style=\"{\r\n              paddingLeft: `${mainPadding}px`,\r\n              width: `${contentWidth2}px`,\r\n            }\"\r\n          >\r\n            <router-view :system-id=\"selectedSysteId\"></router-view>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n \r\n</template>\r\n\r\n<script>\r\n//tree help: https://github.com/vinz3872/vuejs-tree\r\nimport $ from \"jquery\";\r\nimport SystemTree from \"./CDE/SystemTree\";\r\n\r\nexport default {\r\n  components: {\r\n    SystemTree,\r\n  },\r\n  props: [\"dividerPosition\"],\r\n\r\n  data() {\r\n    return {\r\n      hereMoveMainProjectTitle: \"#hereMoveMainProjectTitle\",\r\n      appType: \"\",\r\n      projectTitle: \"\",\r\n      projectNumber: \"\",\r\n      mainTitle: \"\",\r\n      selectedSysteId: \"\",\r\n      selectedFilesType: \"\",\r\n      mainPadding:null,\r\n      contentWidth2:window.innerWidth - $('#sidebar').innerWidth() - this.dividerPosition2,\r\n\r\n      dividerPosition2: 450,\r\n    \r\n    };\r\n  },\r\n  emits: {\r\n    showSearchbar: false,\r\n    appType: localStorage.getItem(\"selectedProjectType\"),\r\n  },\r\n\r\n  methods: {\r\n    folderClick(aObject) {\r\n      /*   this.$emit('folder-click', [{\r\n          'docType':aObject[0].docType,\r\n          'folderId':aObject[0].folderId,\r\n        }])*/\r\n      this.selectedSysteId = aObject[0].folderId;\r\n      this.selectedFilesType = aObject[0].docType;\r\n    },\r\n    /// resizable left sidebar!\r\n    handleDragging2(e) {\r\n\r\n      \r\n      const percentage = (e.pageX /  this.contentWidth2) * 100;\r\n\r\n      if (localStorage.getItem(\"dividerPosition2\")) {\r\n        this.dividerPosition2 = localStorage.getItem(\"dividerPosition2\");\r\n      }\r\n\r\n    //  if (e.pageX + 1 > this.menuMinSize && percentage <= 30) {\r\n        //only resizable if width of the left sidebar is in this percentage range\r\n        this.dividerPosition2 = (percentage / 100) * this.contentWidth2;\r\n   //   }\r\n   \r\n      this.contentWidth2 = window.innerWidth - $('#sidebar').innerWidth() ;\r\n      this.mainPadding = this.dividerPosition2 - $('#sidebar').innerWidth();\r\n\r\n      localStorage.setItem(\"dividerPosition2\", this.dividerPosition2);\r\n\r\n      // -------------------\r\n    },\r\n    startDragging2() {\r\n      document.addEventListener(\"mousemove\", this.handleDragging2);\r\n    },\r\n    endDragging2() {\r\n      document.removeEventListener(\"mousemove\", this.handleDragging2);\r\n    },\r\n\r\n    getDimensions2() {\r\n      //on window resize\r\n      this.contentWidth2 = window.innerWidth - $('#sidebar').innerWidth() - this.dividerPosition2 ;\r\n      \r\n   /*   if (window.innerWidth > 442) {\r\n        this.dividerPosition2 = 300;\r\n        this.contentWidth = window.innerWidth;\r\n        this.isActive = true;\r\n        this.iconCloseOpen = \"&times;\";\r\n        this.isCentered = false;\r\n      } else {\r\n        this.dividerPosition = this.menuMinSize;\r\n        this.isActive = false;\r\n        this.iconCloseOpen = \"&#9776;\";\r\n\r\n        //  this.isCentered = true;\r\n      }*/\r\n\r\n      \r\n    },\r\n    newDimensions(as){\r\n      alert(as)\r\n    },\r\n\r\n\r\n    setToFullScreenWidth() {\r\n    \r\n      this.dividerPosition2 = (this.dividerPosition*1) + 250;\r\n\r\n      this.contentWidth2 = window.innerWidth - $('#sidebar').innerWidth() ;\r\n      this.mainPadding = this.dividerPosition2 - $('#sidebar').innerWidth();\r\n      localStorage.setItem(\"dividerPosition2\", this.dividerPosition2);\r\n     \r\n        \r\n      \r\n      },\r\n  },\r\n\r\n  mounted() {\r\n    this.$emit(\"appType\", localStorage.getItem(\"selectedProjectType\"));\r\n    this.$emit(\"showSearchbar\", false);\r\n    window.addEventListener(\"resize\", this.getDimensions2);\r\n    this.setToFullScreenWidth();\r\n\r\n    this.emitter.on(\"newDimensions\", dividerPosition => {\r\n      this.dividerPosition = dividerPosition;\r\n      setToFullScreenWidth()\r\n    });\r\n\r\n\r\n  },\r\n  unmounted() {\r\n    window.removeEventListener(\"resize\", this.getDimensions2);\r\n  },\r\n\r\n    \r\n\r\n\r\n  created() {\r\n\r\n  this.setToFullScreenWidth();\r\n\r\n\r\n\r\n    //project title... movo to the topBar\r\n    this.projectTitle = localStorage.getItem(\"selectedProjectTitle\");\r\n    this.projectNumber = localStorage.getItem(\"selectedProjectNumber\");\r\n    this.mainTitle = \"<h3 class='text-center'>\" + this.projectTitle + \"</h3>\";\r\n    this.mainTitle += \"<h5 class='text-center'>\" + this.projectNumber + \"</h5>\";\r\n\r\n    //back to project list if no project selected!\r\n    this.appType = localStorage.getItem(\"appType\");\r\n\r\n    if (this.appType == null) {\r\n      this.$router.replace(\"/pages/projectList\");\r\n    }\r\n    //------------------------------------------------\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.wrapper2 {\r\n  /* height: 100vh;*/\r\n  display: flex;\r\n}\r\n\r\n.divider2 {\r\n  height: 100vh;\r\n  width: 4px;\r\n  border-left: 1px solid #ddd;\r\n  transform: translateX(-1px);\r\n  position: absolute;\r\n  top: 0;\r\n  z-index: 1;\r\n  cursor: ew-resize;\r\n  background-color: rgb(255, 255, 255);\r\n}\r\n\r\n.systemsTree {\r\n  position: fixed !important;\r\n}\r\n.divider2 {\r\n  position: fixed !important;\r\n}\r\n\r\n.sidebar {\r\n  background: #f0f0f0;\r\n  font-size: 0.92rem;\r\n  min-width: 32px;\r\n  position: fixed;\r\n  height: calc(100vh - 65px);\r\n}\r\n</style>\r\n\r\n"]},"metadata":{},"sourceType":"module"}