d.length < 4) {\n        this.showPasswordAlert = true;\n        this.formIsValid = false;\n        this.passwordMessage = \"Password is too short\";\n      }\n\n      if (this.password === \"\") {\n        this.showPasswordAlert = true;\n        this.formIsValid = false;\n        this.passwordMessage = \"Please enter password\";\n      }\n\n      if (!this.formIsValid) {\n        return;\n      }\n\n      this.isLoading = true;\n      this.closeModal(\"closeLoginModal\"); //close login modal after submit;\n\n      try {\n        await this.$store.dispatch(\"login\", {\n          username: this.username,\n          password: this.password\n        });\n        this.$router.replace(\"/pages/projectList\");\n      } catch (e) {\n        this.error = e.message || \"Failed to authenticate.\";\n      }\n\n      this.isLoading = false;\n    }\n\n  },\n  emits: [\"set-fullscreenwidth\"],\n\n  mounted() {\n    this.getSettings();\n    this.showSearch = false; //if user is logged in then go to project list!\n\n    if (this.$store.getters.isAuthenticated) {\n      this.$router.replace(\"/pages/projectList\");\n    }\n\n    this.$emit(\"set-fullscreenwidth\"); //  this.getSettings();\n  }\n  /*  async  getSettings(){\r\n            alert(\"d\")\r\n        \r\n            const response = await fetch('https://www.it-b.hr/computech.php?auth=',{\r\n                  method: 'GET'\r\n              });\r\n              \r\n  \r\n              const responseData = await response.json();\r\n            alert(responseData)\r\n      }*/\n\n\n};","map":{"version":3,"mappings":";AAsGA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE,KADP;MAELC,aAAa,EAAE,KAAKC,aAFf;MAGLC,kBAAkB,EAAE,MAHf;MAKLC,QAAQ,EAAE,EALL;MAOL;MACAC,QAAQ,EAAE,EARL;MASLC,QAAQ,EAAE,EATL;MAULC,iBAAiB,EAAE,KAVd;MAWLC,iBAAiB,EAAE,KAXd;MAYLC,WAAW,EAAE,IAZR;MAaLC,SAAS,EAAE,KAbN;MAcLC,KAAK,EAAE,IAdF;MAgBL;MACAC,KAAK,EAAE;IAjBF,CAAP;EAmBD,CArBY;;EAuBbC,OAAO,EAAE;IACP;IACA,MAAMC,WAAN,GAAoB;MAClB,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAAC,gBA