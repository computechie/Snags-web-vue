iption\"\r\n        :disabled=\"thisApp.disabled\"\r\n      />\r\n      </div>\r\n\r\n    </div>\r\n  </div>\r\n  <!--\r\n\r\n  <router-link @click=\"setProjectType('CDE')\" :to=\"'/pages/project/CDE/'+selectedProjectKey\" class=\"card text-white bg-primary mb-3 col-md-3 \">\r\n      <div class=\"card-header\">CDE</div>\r\n      <div class=\"card-body\">\r\n        <h5 class=\"card-title\">Common Data Environment</h5>\r\n        <p class=\"card-text\">Some quick example text to build on the card title and make up the bulk of the card's content.</p>\r\n      </div>\r\n  </router-link  >-->\r\n</template>\r\n\r\n<script>\r\nimport moment from \"moment\";\r\nimport AvailableApps from \"../ui/AvailableApps.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    AvailableApps,\r\n  },\r\n  data() {\r\n    return {\r\n      selectedProjectTitle: \",\",\r\n      selectedProjectNumber: \",\",\r\n      selectedProjectContractor: \",\",\r\n      selectedProjectValidUntil: \",\",\r\n      selectedProjectPublishDate: \",\",\r\n      selectedProjectImage: \",\",\r\n      selectedProjectType: \",\",\r\n      selectedProjectKey: \",\",\r\n      appType: \"\",\r\n\r\n      allApps: [],\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    format_date(value, pattern) {\r\n      if (value) {\r\n        return moment(String(value)).format(pattern);\r\n      }\r\n    },\r\n    setProjectType(whatType) {\r\n      localStorage.setItem(\"appType\", whatType);\r\n      localStorage.setItem(\"selectedProjectType\", whatType);\r\n    },\r\n  },\r\n  created() {\r\n    localStorage.setItem(\"selectedProjectType\", \"\");\r\n\r\n    this.selectedProjectKey = localStorage.getItem(\"projectKey\");\r\n    this.selectedProjectTitle = localStorage.getItem(\"selectedProjectTitle\");\r\n    this.selectedProjectNumber = localStorage.getItem(\"selectedProjectNumber\");\r\n    this.selectedProjectContractor = localStorage.getItem(\r\n      \"selectedProjectContractor\"\r\n    );\r\n    this.selectedProjectValidUntil = this.format_date(\r\n      localStorage.getItem(\"selectedProjectValidUntil\"),\r\n      \"DD/MM/YYYY\"\r\n    );\r\n    this.selectedProjectPublishDate = this.format_date(\r\n      localStorage.getItem(\"selectedProjectPublishDate\"),\r\n      \"DD/MM/YYYY\"\r\n    );\r\n    this.selectedProjectImage = localStorage.getItem(\"selectedProjectImage\");\r\n\r\n    this.allApps = [\r\n      {\r\n        id: \"1\",\r\n        appName: \"CDE\",\r\n        appShortcode: \"CDE\",\r\n        appSubTitle: \"Common Data Environment\",\r\n        applink: \"applink\",\r\n        appDescription: \"appDescription\",\r\n        disabled: \"false\",\r\n        backgroundColor: \"#0d6efd\",\r\n        textColor: \"#ffffff\",\r\n      },\r\n      {\r\n        id: \"2\",\r\n        appName: \"DFM Uploader\",\r\n        appShortcode: \"DFMuploader\",\r\n        appSubTitle: \"DFM Uploader\",\r\n        applink: \"applink\",\r\n        appDescription: \"appDescription\",\r\n        disabled: \"false\",\r\n        backgroundColor: \"#0d6efd\",\r\n        textColor: \"#ffffff\",\r\n      },\r\n      {\r\n        id: \"3\",\r\n        appName: \"QMS\",\r\n        appShortcode: \"QMS\",\r\n        appSubTitle: \"Quality Management System\",\r\n        applink: \"applink\",\r\n        appDescription: \"appDescription\",\r\n        disabled: \"false\",\r\n        backgroundColor: \"#0d6efd\",\r\n        textColor: \"#ffffff\",\r\n      },\r\n      {\r\n        id: \"4\",\r\n        appName: \"Safety File\",\r\n        appShortcode: \"SafetyFile\",\r\n        appSubTitle: \"Safety File\",\r\n        applink: \"applink\",\r\n        appDescription: \"appDescription\",\r\n        disabled: \"false\",\r\n        backgroundColor: \"#6c757d\",\r\n        textColor: \"#ffffff\",\r\n      },\r\n      {\r\n        id: \"5\",\r\n        appName: \"PPM\",\r\n        appShortcode: \"PPM\",\r\n        appSubTitle: \"PPM\",\r\n        applink: \"applink\",\r\n        appDescription: \"appDescriptio