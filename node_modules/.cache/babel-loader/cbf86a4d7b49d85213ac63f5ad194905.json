{"ast":null,"code":"export default {\n  data() {\n    return {\n      settingsJson: \"\",\n      //login:\n      username: \"\",\n      password: \"\",\n      showUsernameAlert: false,\n      showPasswordAlert: false,\n      formIsValid: true,\n      isLoading: false,\n      error: null,\n      //testing;\n      token: \"\"\n    };\n  },\n\n  methods: {\n    usernameKeyDown() {\n      this.showUsernameAlert = false;\n\n      if (this.username.length == 0) {\n        this.showUsernameAlert = true;\n        this.formIsValid = false;\n        this.usernameMessage = \"Please enter username\";\n      }\n    },\n\n    passwordKeyDown() {\n      this.showPasswordAlert = false;\n    },\n\n    closeModal(id) {\n      document.getElementById(id).click();\n    },\n\n    async submitForm() {\n      this.showUsernameAlert = false;\n      this.showPasswordAlert = false;\n      this.formIsValid = true;\n      this.usernameMessage = \"\";\n      this.passwordMessage = \"\";\n\n      if (this.username === \"\") {\n        this.showUsernameAlert = true;\n        this.formIsValid = false;\n        this.usernameMessage = \"Please enter username\";\n      }\n\n      if (this.password.length < 4) {\n        this.showPasswordAlert = true;\n        this.formIsValid = false;\n        this.passwordMessage = \"Password is too short\";\n      }\n\n      if (this.password === \"\") {\n        this.showPasswordAlert = true;\n        this.formIsValid = false;\n        this.passwordMessage = \"Please enter password\";\n      }\n\n      if (!this.formIsValid) {\n        return;\n      }\n\n      this.isLoading = true;\n      this.closeModal(\"closeLoginModal\"); //close login modal after submit;\n\n      try {\n        await this.$store.dispatch(\"login\", {\n          username: this.username,\n          password: this.password\n        });\n        this.$router.replace(\"/pages/projectList\");\n      } catch (e) {\n        this.error = e.message || \"Failed to authenticate.\";\n      }\n\n      this.isLoading = false;\n    }\n\n  },\n  emits: [\"set-fullscreenwidth\"],\n\n  mounted() {\n    //if user is logged in then go to project list!\n    if (this.$store.getters.isAuthenticated) {\n      this.$router.replace(\"/pages/projectList\");\n    }\n\n    this.$emit(\"set-fullscreenwidth\"); //  this.getSettings();\n  }\n  /*  async  getSettings(){\r\n            alert(\"d\")\r\n        \r\n            const response = await fetch('https://www.it-b.hr/computech.php?auth=',{\r\n                  method: 'GET'\r\n              });\r\n              \r\n  \r\n              const responseData = await response.json();\r\n            alert(responseData)\r\n      }*/\n\n\n};","map":{"version":3,"mappings":"AAuFA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,EADT;MAGL;MACAC,QAAQ,EAAE,EAJL;MAKLC,QAAQ,EAAE,EALL;MAMLC,iBAAiB,EAAE,KANd;MAOLC,iBAAiB,EAAE,KAPd;MAQLC,WAAW,EAAE,IARR;MASLC,SAAS,EAAE,KATN;MAULC,KAAK,EAAE,IAVF;MAYL;MACAC,KAAK,EAAE;IAbF,CAAP;EAeD,CAjBY;;EAmBbC,OAAO,EAAE;IACPC,eAAe,GAAG;MAChB,KAAKP,iBAAL,GAAyB,KAAzB;;MACA,IAAI,KAAKF,QAAL,CAAcU,MAAd,IAAwB,CAA5B,EAA+B;QAC7B,KAAKR,iBAAL,GAAyB,IAAzB;QACA,KAAKE,WAAL,GAAmB,KAAnB;QACA,KAAKO,eAAL,GAAuB,uBAAvB;MACF;IACD,CARM;;IAUPC,eAAe,GAAG;MAChB,KAAKT,iBAAL,GAAyB,KAAzB;IACD,CAZM;;IAcPU,UAAU,CAACC,EAAD,EAAK;MACbC,QAAQ,CAACC,cAAT,CAAwBF,EAAxB,EAA4BG,KAA5B;IACD,CAhBM;;IAkBP,MAAMC,UAAN,GAAmB;MACjB,KAAKhB,iBAAL,GAAyB,KAAzB;MACA,KAAKC,iBAAL,GAAyB,KAAzB;MACA,KAAKC,WAAL,GAAmB,IAAnB;MACA,KAAKO,eAAL,GAAuB,EAAvB;MACA,KAAKQ,eAAL,GAAuB,EAAvB;;MAEA,IAAI,KAAKnB,QAAL,KAAkB,EAAtB,EAA0B;QACxB,KAAKE,iBAAL,GAAyB,IAAzB;QACA,KAAKE,WAAL,GAAmB,KAAnB;QACA,KAAKO,eAAL,GAAuB,uBAAvB;MACF;;MAEA,IAAI,KAAKV,QAAL,CAAcS,MAAd,GAAuB,CAA3B,EAA8B;QAC5B,KAAKP,iBAAL,GAAyB,IAAzB;QACA,KAAKC,WAAL,GAAmB,KAAnB;QACA,KAAKe,eAAL,GAAuB,uBAAvB;MACF;;MAEA,IAAI,KAAKlB,QAAL,KAAkB,EAAtB,EAA0B;QACxB,KAAKE,iBAAL,GAAyB,IAAzB;QACA,KAAKC,WAAL,GAAmB,KAAnB;QACA,KAAKe,eAAL,GAAuB,uBAAvB;MACF;;MAEA,IAAI,CAAC,KAAKf,WAAV,EAAuB;QACrB;MACF;;MAEA,KAAKC,SAAL,GAAiB,IAAjB;MACA,KAAKQ,UAAL,CAAgB,