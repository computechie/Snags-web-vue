, 2);\n      return year + '-' + month + '-' + day + \" \" + aTime;\n    },\n\n    getTrips() {\n      this.isLoading = true;\n      const fromDateFull = this.format_date_to_full(this.fromDate, '00:00:00');\n      const toDateFull = this.format_date_to_full(this.toDate, '23:59:59');\n      const sessionId = localStorage.getItem(\"token\");\n      const DateFrom = fromDateFull;\n      const DateTo = toDateFull;\n      const UserId = localStorage.getItem(\"userId\");\n      const UserType = this.userType;\n      const baseUrl = localStorage.getItem(\"_rootRestUrl\");\n      let formData = JSON.stringify({\n        sessionId: sessionId,\n        DateFrom: DateFrom,\n        DateTo: DateTo,\n        UserId: UserId,\n        UserType: UserType\n      });\n      let config = {\n        headers: {\n          \"Content-Type\": \"application/json\"\n        }\n      };\n      axios.get(baseUrl + \"/api/v1/Trip/Trips/?sessionId=\" + sessionId + \"&DateFrom=\" + DateFrom + \"&DateTo=\" + DateTo + \"&UserId=\" + UserId + \"&UserType=\" + UserType + \"\", formData, config).then(response => {\n        console.table(response.data);\n        this.allTrips = response.data;\n        this.isLoading = false;\n      }).catch(function (error) {\n        // handle error\n        console.log(error);\n        this.isLoading = false;\n      });\n    },\n\n    onRowSelect(event) {\n      console.table(event.originalEvent.target);\n      this.getDocumentDetails(event.data.id);\n    },\n\n    onRowUnselect(event) {\n      console.log(event);\n    },\n\n    getDocumentDetails(documentKey) {\n      //load from api them\n      this.openPanel(documentKey);\n    },\n\n    openPanel(documentKey) {\n      alert(documentKey);\n    },\n\n    initFilters1() {\n      this.filters1 = {\n        'global': {\n          value: null,\n          matchMode: FilterMatchMode.CONTAINS\n        },\n        'userFullName': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,