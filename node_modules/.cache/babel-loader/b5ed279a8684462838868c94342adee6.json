this.isLoading = true;\r\n\r\n      fetch(\"https://jsonplaceholder.typicode.com/users\")\r\n        .then((response) => response.json())\r\n        .then((data) => {\r\n          this.users = data;\r\n          setTimeout(() => {\r\n            $(\"#datatable\").DataTable({\r\n              lengthMenu: [\r\n                [5, 10, 25, 50, -1],\r\n                [5, 10, 25, 50, \"All\"],\r\n              ],\r\n              pageLength: 5,\r\n            });\r\n          });\r\n        });\r\n\r\n      this.isLoading = false;\r\n    },\r\n  },\r\n\r\n  emits: [\"showSeachbar\"],\r\n\r\n  mounted() {\r\n   \r\n    this.getUsers();\r\n    // this.$emit(\"showSearchbar\", true);\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n@import \"datatables.net-dt\";\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}