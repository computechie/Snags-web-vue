his.selectedProjectImage = localStorage.getItem(\"selectedProjectImage\");\r\n   \r\n  },\r\n\r\n  emits: {\r\n    showSearchbar: false,\r\n  },\r\n\r\n  mounted() {\r\n    this.$emit(\"showSearchbar\", false);\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n.projectDetailstable {\r\n  border: 1px solid #ccc;\r\n}\r\n.projectDetailstable td {\r\n  border-bottom: 1px solid #eee;\r\n}\r\n\r\n.selectAppButton{display:block;width:200px;color:white !important;margin-top:0.5rem\r\n}\r\n\r\n.disabled {cursor: not-allowed !important; pointer-events: auto !important;background-color:#cccccc !important}\r\n</style>"]},"metadata":{},"sourceType":"module"}